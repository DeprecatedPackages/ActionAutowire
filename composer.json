{
    "name": "symplify/action-autowire",
    "description": "Adds support to autowire controller action.",
    "keywords": ["Symfony", "controller", "autowire", "action"],
    "license": "MIT",
    "require": {
        "php": "~7.0",
        "symfony/dependency-injection": "^3.1",
        "symfony/http-kernel": "^3.1",
        "symfony/yaml": "^3.1",
        "symfony/property-access": "^3.1"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "phpunit/phpunit": "^5.5",
        "symfony/framework-bundle": "^3.1",
        "symplify/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ActionAutowire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ActionAutowire\\Tests\\": "tests"
        }
    }
}
