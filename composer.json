{
    "name": "symplify/action-autowire",
    "description": "Adds support to autowire action arguments without pain.",
    "keywords": ["Symfony", "controller", "autowire"],
    "license": "MIT",
    "require": {
        "php": "~7.0",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/http-kernel": "^3.1",
        "symfony/yaml": "^3.1"
    },
    "require-dev": {
        "nette/utils": "^2.4",
        "symfony/framework-bundle": "^3.0",
        "symplify/coding-standard": "^0.3",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ActionAutowire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ActionAutowire\\Tests\\": "tests"
        }
    }
}
